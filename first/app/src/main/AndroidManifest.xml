<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fernandosousa.my_first_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="br.com.fernandosousa.my_first_app.My_first_app"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="br.com.fernandosousa.my_first_app.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="br.com.fernandosousa.my_first_app.DebugActivity" />
        <activity
            android:name="br.com.fernandosousa.my_first_app.TelaInicialActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="br.com.fernandosousa.my_first_app.DisciplinaActivity"
            android:parentActivityName="br.com.fernandosousa.my_first_app.TelaInicialActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity android:name="br.com.fernandosousa.my_first_app.DisciplinaCadastroActivity"></activity>
    </application>

</manifest>